{
  "name": "webjamsocketserver",
  "description": "Uses latest version of socketcluster-server",
  "version": "2.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=18.15.0 <19",
    "npm": ">=9.5.0 <10"
  },
  "contributors": [
    {
      "name": "Jonathan Gros-Dubois",
      "email": "grosjona@yahoo.com.au"
    },
    {
      "name": "Joshua V Sherman",
      "email": "joshua.v.sherman@gmail.com"
    }
  ],
  "keywords": [
    "websocket",
    "server",
    "realtime",
    "cluster",
    "scalable",
    "typescript"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "rmrf": "rm -rf build && rm -rf JaMmusic && rm -rf coverage",
    "start:debug": "rm -rf build && tsc && npm-run-all -p ts-start ts-watch",
    "test": "eslint . && rm -rf coverage && npm run test:jest",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "build:front": "./postinstallJaM.sh && rm -rf ./JaMmusic/src && rm -rf ./JaMmusic/test",
    "build:prod": "(cd JaMmusic || exit; npm start -- build)",
    "cleaninstall": "rm -rf yarn.lock && rm -rf package-lock.json && rm -rf node_modules && npm cache clean --force && npm install",
    "start": "node build/index.js",
    "postinstall": "npm run rmrf && tsc && npm run build:front",
    "ts-start": "DEBUG=WebJamSocketServer:* nodemon build/index.js",
    "ts-watch": "tsc -w",
    "test:lint": "eslint . --fix",
    "test:local": "eslint . --fix && npm run test:jest && npm run cc",
    "test:jest": "jest --config jest.config.json --runInBand --forceExit"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/eetase": "^4.0.2",
    "@types/express": "^4.17.17",
    "@types/express-sslify": "^1.2.2",
    "@types/morgan": "^1.9.4",
    "@types/scc-broker-client": "^8.0.0",
    "@types/socketcluster-server": "^17.3.0",
    "@types/superagent": "^4.1.16",
    "@types/uuid": "^9.0.1",
    "connect": "^3.7.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eetase": "^5.0.0",
    "express": "^4.18.2",
    "express-sslify": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0",
    "sc-errors": "^2.0.1",
    "scc-broker-client": "^8.2.0",
    "serve-static": "^1.15.0",
    "snyk": "^1.1114.0",
    "socketcluster-client": "^17.1.1",
    "socketcluster-server": "^17.3.1",
    "superagent": "^8.0.9",
    "supports-color": "^9.3.1",
    "typescript": "^4.9.5",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5"
  }
}
